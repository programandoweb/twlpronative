{
  "expo": {
    "name": "TwlPro",
    "slug": "twlpro-native",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "ios": {
      "bundleIdentifier": "com.twlpro.chat",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.twlpro.chat",
      "versionCode": 1,
    },
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "permissions": [
      "CAMERA",
      "MANAGE_DOCUMENTS",
      "READ_EXTERNAL_STORAGE",
      "WRITE_EXTERNAL_STORAGE",
      "RECORD_AUDIO",
      "WRITE_EXTERNAL_STORAGE",
      "com.anddoes.launcher.permission.UPDATE_COUNT",
    ],
  }
}
